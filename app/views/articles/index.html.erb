<h1> Here are the articles </h1>
<table>
  
  

<tr> 
<th> Title </th>
<th> Description</th>
<th> Show </th>
<th> Edit </th>
</tr>

<%  @articles.each do |article| %>
<tr> 
<td> <%= article.title %> </td>
<td> <%= article.description %> </td>
<td> <%= link_to 'show', article_path(article) %> </td>
<td> <%= link_to 'edit', edit_article_path(article) %> </td>

</tr>
<% end %>
</table>